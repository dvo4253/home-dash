{
  "name": "home-dash-client",
  "version": "1.0.0",
  "description": "Template for react 16 ssr app",
  "repository": "https://github.com/dvo4253/react16-ssr.git",
  "scripts": {
    "predev": "rm -rf ./dist",
    "dev": "npm run lint && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:analyze": "BUNDLE_ANALYZER=true npm run dev",
    "dev:server": "NODE_ENV=development BABEL_ENV=development nodemon src/server/index.js --exec babel-node",
    "dev:client": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config webpack/webpack.dev.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack/webpack.prod.babel.js",
    "build:server": "webpack --config webpack/webpack.server.babel.js",
    "start": "node ./dist/server/index.js",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && npm install && npm outdated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@material-ui/core": "1.5.0",
    "@material-ui/icons": "2.0.3",
    "assets-webpack-plugin": "3.9.7",
    "autoprefixer": "9.4.7",
    "axios": "0.18.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "body-parser": "1.18.3",
    "concurrently": "4.1.0",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "csurf": "1.9.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "express": "4.16.4",
    "express-winston": "3.0.1",
    "helmet": "3.15.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-pug-plugin": "0.3.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.0.0",
    "material-design-icons": "3.0.1",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "prop-types": "15.6.2",
    "pug": "2.0.3",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "reducers": "3.0.0-alpha",
    "redux": "4.0.1",
    "redux-observable": "1.0.0",
    "rxjs": "6.4.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.1",
