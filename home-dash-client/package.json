{
  "name": "home-dash-client",
  "version": "1.0.0",
  "description": "Template for react 16 ssr app",
  "repository": "https://github.com/dvo4253/react16-ssr.git",
  "scripts": {
    "predev": "rm -rf ./dist",
    "dev": "npm run lint && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:analyze": "BUNDLE_ANALYZER=true npm run dev",
    "dev:server": "wait-on -d 10000 -w 2000 ./dist/public && npm run dev:server:run",
    "dev:server:run": "concurrently \"npm run dev:server:build\" \"npm run dev:server:start\"",
    "dev:server:build": "webpack -d --watch --config webpack/webpack.server.babel.js",
    "dev:server:start": "wait-on -d 10000 -w 2000 ./dist/server && nodemon start || true",
    "dev:client": "webpack-dev-server --watch --config webpack/webpack.dev.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack/webpack.prod.babel.js",
    "build:server": "webpack --config webpack/webpack.server.babel.js",
    "start": "node ./dist/server/index.js",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && rm -f package-lock.json && npm install && npm outdated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@babel/runtime": "7.4.5",
    "@material-ui/core": "4.0.2",
    "@material-ui/icons": "4.0.1",
    "assets-webpack-plugin": "3.9.10",
    "autoprefixer": "9.6.0",
    "axios": "0.21.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "body-parser": "1.19.0",
    "concurrently": "4.1.0",
    "cookie-parser": "1.4.4",
    "cookie-session": "2.0.0-beta.3",
    "core-js": "3.1.3",
    "csurf": "1.10.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "express": "4.17.1",
    "express-winston": "3.1.0",
    "helmet": "3.18.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-pug-plugin": "1.0.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.8.0",
    "jsonwebtoken": "8.5.1",
    "material-design-icons": "3.0.1",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "prop-types": "15.7.2",
    "pug": "2.0.3",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "reducers": "3.0.0-alpha",
