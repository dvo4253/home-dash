    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "typeface-roboto": "0.0.54",
    "webpack": "4.33.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-disk-plugin": "0.0.2",
