{
  "name": "home-dash-server",
  "version": "1.0.0",
  "description": "Home Dashboard Server",
  "scripts": {
    "babel-node": "babel-node ./src/index.js",
    "predev": "rm -rf dist",
    "dev": "npm run lint && npm run dev:server",
    "dev:server": "nodemon --exec babel-node -- src/index.js ",
    "start:inspect": "npm run lint && npm run build && nodemon --inspect --exec npm run babel-node",
    "build": "webpack --config webpack/webpack.server.babel.js",
    "start": "node ./dist/server/index.js",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "express": "4.16.4",
    "express-winston": "3.0.1",
    "helmet": "3.15.0",
    "prom-client": "11.2.1",
    "querystring": "0.2.0",
    "ramda": "0.26.1",
    "webpack": "4.28.4",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.5",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  }
}
