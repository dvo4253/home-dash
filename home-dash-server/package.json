{
  "name": "home-dash-server",
  "version": "1.0.0",
  "description": "Home Dashboard Server",
  "scripts": {
    "babel-node": "babel-node ./src/index.js",
    "build": "webpack --config webpack/webpack.server.babel.js",
    "build:server:dev": "webpack --config webpack/webpack.server.babel.js",
    "dev": "npm run lint && npm run build:server:dev && npm run dev:server",
    "dev:server": "nodemon src/index.js --exec babel-node",
    "start": "node ./dist/server/index.js",
    "start:dev": "wait-on ./dist/server && nodemon --watch ./dist/server",
    "start:inspect": "npm run lint && npm run build && nodemon --inspect --exec npm run babel-node",
    "test": "npm run lint && jest --watch",
    "test:ci": "npm run lint && jest --coverage",
    "lint": "eslint --fix ./",
    "init": ". init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assets-webpack-plugin": "3.8.3",
    "axios": "0.18.0",
    "babel-loader": "7.1.4",
    "body-parser": "1.18.3",
    "clean-webpack-plugin": "0.1.19",
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "express-winston": "2.5.1",
    "helmet": "3.15.0",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-pug-plugin": "0.3.0",
    "prom-client": "11.1.3",
    "querystring": "0.2.0",
    "ramda": "0.25.0",
    "source-map-support": "0.5.9",
    "webpack": "4.12.0",
    "webpack-disk-plugin": "0.0.2",
    "webpack-merge": "4.1.3",
    "webpack-node-externals": "1.7.2",
    "winston": "2.4.3",
    "write-assets-webpack-plugin": "1.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "nodemon": "1.17.5",
    "style-loader": "0.21.0",
    "wait-on": "2.1.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
